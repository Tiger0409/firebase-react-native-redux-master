{
  "extends": "airbnb",
  "env": {
    "node": true
  },
  "rules": {
    "func-names": 0,
    "no-underscore-dangle": 0,
    "max-len": 0,
    "no-class-assign": 0,
    "react/no-unescaped-entities": 0,
    "jsx-a11y/no-static-element-interactions" : 0,
    "react/jsx-filename-extension" : 0,
    "no-use-before-define" : 0
  }
}